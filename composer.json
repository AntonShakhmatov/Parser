{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
		"php": ">=7.3",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.4",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"latte/latte": "^2.9",
		"tracy/tracy": "^2.8",
		"guzzlehttp/guzzle": "^7.4",
      "ext-json": "*",
		"psr/http-message": "^1.0",
      "ext-curl": "*",
		"paquettg/php-html-parser": "^3.1",
		"curl/curl": "^2.3",
      "ext-dom": "*",
		"cagartner/phpquery": "^0.9.13",
		"sunra/php-simple-html-dom-parser": "^1.5",
		"php-webdriver/webdriver": "^1.12",
		"irazasyed/telegram-bot-sdk": "3.4.1",
		"doctrine/annotations": "^1.13"
    },
	"require-dev": {
		"nette/tester": "^2.4",
		"symfony/thanks": "^1",
		"phpstan/phpstan-nette": "^1.0",
		"phpunit/phpunit": "^9.5",
		"phpunit/phpunit-selenium": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Src\\": [
				"src/"
			]
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	},
	"scripts": {
		"serve": "php -s 0.0.0.0:8000 -t public index.php",
		"test": "phpunit --colors=always"
	}
}
